{
  "name": "react-webpack5-boilerplate",
  "version": "0.0.1",
  "description": "A react webpack5 boilerplate",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.conf.js",
    "build": "webpack --config config/webpack.prod.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactjser/react-webpack5-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack5",
    "boilerplate"
  ],
  "author": "reactjser <reactjser@163.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactjser/react-webpack5-boilerplate/issues"
  },
  "homepage": "https://github.com/reactjser/react-webpack5-boilerplate#readme",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "address": "^1.1.2",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.1.3",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.3.1",
    "lint-staged": ">=10",
    "mini-css-extract-plugin": "^1.3.9",
    "portfinder": "^1.0.28",
    "postcss": "^8.2.8",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "simple-git-hooks": ">=2.0.3",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpackbar": "^5.0.0-3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  }
}
